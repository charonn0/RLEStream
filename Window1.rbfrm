#tag Events PushButton1
	#tag Event
		Sub Action()
		  Dim data As New MemoryBlock(1024)
		  Dim r As New RLEStream(data)
		  Dim m As New BinaryStream(TextArea1.Text)
		  While Not m.EOF
		    r.Write(m.Read(1))
		  Wend
		  r.Flush
		  r.Close
		  m.Close
		  TextArea2.Text = data
		End Sub
	#tag EndEvent
#tag EndEvents
